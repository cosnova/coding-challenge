{
  "scripts": {
    "prettier": "prettier --config .prettierrc --write",
    "eslint": "eslint ./web ./graphql"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-vue": "^5.1.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "1.16.1",
    "vue-eslint-parser": "^5.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.*": [
      "npm run prettier",
      "git add"
    ],
    "*.{js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.10.2"
  }
}
